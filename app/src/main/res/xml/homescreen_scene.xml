<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4168">
       <KeyFrameSet>
           <KeyPosition
               motion:motionTarget="@+id/__img_group_25"
               motion:framePosition="80"
               motion:keyPositionType="parentRelative"
               motion:percentY="-0.1" />
           <KeyPosition
               motion:motionTarget="@+id/ellipse_1"
               motion:framePosition="70"
               motion:keyPositionType="parentRelative"
               motion:percentY="0.60005" />
           <KeyPosition
               motion:motionTarget="@+id/group_1"
               motion:framePosition="50"
               motion:keyPositionType="parentRelative"
               motion:percentY="0.8" />
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="23.74"
            motion:maxVelocity="3.05"
            motion:springDamping="32.061"
            motion:springStiffness="40.0"
            motion:springStopThreshold="1.0E-4"
            motion:springMass="0.9" />
    </Transition>
    <Transition
        motion:autoTransition="animateToStart"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4707">
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/group_1"
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:percentY="0.8" />
            <KeyPosition
                motion:motionTarget="@+id/__img_group_25"
                motion:framePosition="80"
                motion:keyPositionType="parentRelative"
                motion:percentY="-0.1" />
            <KeyPosition
                motion:motionTarget="@+id/ellipse_1"
                motion:framePosition="70"
                motion:keyPositionType="parentRelative"
                motion:percentY="0.60005" />
        </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="23.74"
            motion:maxVelocity="3.05"
            motion:springDamping="32.061"
            motion:springStiffness="40.0"
            motion:springStopThreshold="1.0E-4"
            motion:springMass="0.9" />
    </Transition>


    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_width="405dp"
            android:layout_height="866dp"
            android:id="@+id/__img_group_25"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="848dp"
            android:layout_width="495dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end"></ConstraintSet>
</MotionScene>